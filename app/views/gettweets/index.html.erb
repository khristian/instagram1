<%- model_class = Gettweet -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:date) %></th>
      <th><%= model_class.human_attribute_name(:tweet) %></th>
      <th><%= model_class.human_attribute_name(:category) %></th>
      <th><%= model_class.human_attribute_name(:tags) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @gettweets.each do |gettweet| %>
      <tr>
        <td><%= link_to gettweet.id, gettweet_path(gettweet) %></td>
        <td><%= gettweet.tweet %></td>
        <td><%= gettweet.category %></td>
        <td><%= gettweet.date %></td>
        <td><%= gettweet.user_id %></td>
        <td><%= gettweet.store_id %></td>
        <td><%= gettweet.tags %></td>
        <td><%=l gettweet.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_gettweet_path(gettweet), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      gettweet_path(gettweet),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_gettweet_path,
            :class => 'btn btn-primary' %>
